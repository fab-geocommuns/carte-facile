{
  "name": "carte-facile",
  "version": "0.3.1",
  "description": "Une librairie de styles de cartes et surcouches prêtes à l'emploi, pour ajouter des cartes rapidement à votre application avec MapLibre ou d'autres librairies cartographiques.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "npm run build"
  },
  "keywords": [
    "maplibre",
    "map",
    "styles",
    "components",
    "ign",
    "osm"
  ],
  "author": "Carte facile, IGN",
  "license": "Apache-2.0",
  "peerDependencies": {
    "maplibre-gl": "^5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "rollup": "^4.0.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  }
}
